{
  "name": "takehome-task",
  "version": "0.0.1",
  "description": "Wander Backend Developer Test",
  "main": "bootstrap.js",
  "scripts": {
    "test": "jest --coverage --testPathPattern='\\.unit\\.test\\.ts$'",
    "test:integration": "./Scripts/run-integration.sh",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate",
    "prebuild": "rm -rf ./dist",
    "clean": "rm -rf ./dist",
    "docker:up": "docker-compose up -d",
    "build": "tspc",
    "start": "NODE_ENV=production; node dist/bootstrap.js",
    "dev": "prisma migrate dev; ts-node-dev --respawn --transpile-only --no-notify -r tsconfig-paths/register ./bootstrap.ts"
  },
  "keywords": [
    "Caching",
    "SQLite",
    "Prisma"
  ],
  "author": "Femi Olatubosun",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/luxon": "^3.4.2",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^20.12.7",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.6",
    "module-alias": "^2.2.3",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "prisma": "^5.12.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "ts-patch": "^3.1.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "typescript-transform-paths": "^3.4.7"
  },
  "dependencies": {
    "@prisma/client": "^5.12.1",
    "axios": "^1.6.8",
    "body-parser": "^1.20.2",
    "bullmq": "^5.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "luxon": "^3.4.4",
    "winston": "^3.13.0"
  },
  "targets": {
    "build": {
      "options": {
        "assets": [],
        "additionalEntryPoints": [
          {
            "entryName": "sync.processor",
            "entryPath": "Jobs/SearchFailedWeatherInformationJob.ts"
          }
        ]
      }
    }
  }
